{
  "env": { 
      "browser": true,
      "es2021": true 
  },
  "extends": [ 
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
  ],
  "parser": "babel-eslint", 
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 11,
      "sourceType": "module"
  },
  "plugins": [
      "react", 
      "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
      "react/prop-types": 0 ,
      "react/display-name": "off"
  }
}